<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
  <span class="user">
  	<%= link_to feed_item.user.name, feed_item.user %>
  </span>
  <span class="title">
    <%= link_to feed_item.title, feed_item %></span>
  <span class="date">
  	<%= feed_item.date_of_hike.to_formatted_s(:long_ordinal) %>
  </span>
  <span class="length">
  	<%= feed_item.length_of_hike %> Miles
  </span>
</li>